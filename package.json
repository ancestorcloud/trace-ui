{
  "name": "trace-ui",
  "version": "2.5.1",
  "description": "reusable components made w/ <3 by Trace",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel --copy-files --out-dir dist packages",
    "prepublish": "npm run build",
    "postpublish": "rm -rf dist",
    "prebuild": "rm -rf dist && mkdir dist",
    "test": "../../node_modules/.bin/jest --watch",
    "storybook": "start-storybook -p 9002 -c .storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ancestorcloud/trace-ui.git"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-cli": "^6.24.1",
    "babel-jest": "17.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1"
  },
  "peerDependencies": {
    "glamor": "2.20.15",
    "react-modal": "1.5.2",
    "react-ditto": "1.3.0"
  },
  "keywords": [
    "ui",
    "components",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ancestorcloud/trace-ui/issues"
  },
  "homepage": "https://github.com/ancestorcloud/trace-ui#readme",
  "jest": {
    "testRegex": "(test|spec).(js)$"
  },
  "standard": {
    "showEslintRules": true,
    "parser": "babel-eslint",
    "rules": {
      "strict": 0
    }
  },
  "dependencies": {
    "glamor": "^2.20.25",
    "react": "^15.6.1",
    "react-ditto": "^1.5.0",
    "react-dom": "^15.6.1"
  }
}
